<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="640.0" prefWidth="887.0" xmlns="http://javafx.com/javafx/17"
            fx:controller="application.Controllers.Admin.AllOrdersController">
    <TableView fx:id="ordersTableView" layoutX="14.0" layoutY="90.0" prefHeight="537.0" prefWidth="1004">
        <columns>

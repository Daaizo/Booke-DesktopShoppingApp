            <TableColumn fx:id="ordersDateColumn" prefWidth="151.0" resizable="false" text="Order Date"/>
            <TableColumn fx:id="ordersDeliveryDateColumn" prefWidth="176.0" resizable="false" text="Delivery date"/>
            <TableColumn fx:id="ordersPriceColumn" prefWidth="87.0" resizable="false" text="Total price"/>

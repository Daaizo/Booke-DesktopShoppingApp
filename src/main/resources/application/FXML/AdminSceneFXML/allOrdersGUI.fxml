                         text="Payment method"/>
            <TableColumn fx:id="ordersStatusColumn" prefWidth="46.0" resizable="false" text="Status"/>
            <TableColumn fx:id="ordersDetailButtonColumn" minWidth="7.0" prefWidth="7.0" resizable="false"/>
            <TableColumn fx:id="ordersAcceptColumn" prefWidth="316.0" resizable="false"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<Pane xmlns:fx="http://javafx.com/fxml/1" fx:id="favouritesPane" prefHeight="548.0" prefWidth="1050.0"
      xmlns="http://javafx.com/javafx/17"
      fx:controller="application.Controllers.Client.AccountDetails.ClientFavourites">
   <TableView fx:id="favouritesTableView" layoutX="113.0" layoutY="126.0" prefHeight="350.0" prefWidth="820.0">
      <columns>
         <TableColumn fx:id="favouritesNameColumn" editable="false" prefWidth="262.0" resizable="false" sortable="false"
                      text="Name"/>
         <TableColumn fx:id="favouritesPriceColumn" editable="false" prefWidth="97.0" resizable="false" sortable="false"
                      text="Price"/>
         <TableColumn fx:id="favouritesSubcategoryColumn" editable="false" prefWidth="167.0" resizable="false"
                      sortable="false" text="Subcategory"/>
         <TableColumn fx:id="favouritesButtonColumn" editable="false" prefWidth="293.0" resizable="false"
                      sortable="false"/>
      </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
      </columnResizePolicy>
   </TableView>
   <Button layoutX="604.0" layoutY="518.0" mnemonicParsing="false" onAction="#deleteAllFavouritesButtonClicked"
           prefHeight="51.0" prefWidth="399.0" styleClass="DeleteButtons" text="Delete all favorites"/>
   <Button layoutX="80.0" layoutY="518.0" mnemonicParsing="false" onAction="#addAllFavouritesToCartButtonClicked"
           prefHeight="51.0" prefWidth="399.0" styleClass="OrangeButtons" text="Add all favourites to shopping cart"/>
   <Label id="titleLabel" layoutX="181.0" layoutY="36.0" prefHeight="68.0" prefWidth="738.0" style="-fx-font-size: 55;"
          text="List of all favourite products"/>
</Pane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<Pane xmlns:fx="http://javafx.com/fxml/1" fx:id="ebooksPane" xmlns="http://javafx.com/javafx/17"
      fx:controller="application.Controllers.Client.Products.AllEbooksTableController">
   <children>
      <TableView id="ebooksTableView" fx:id="ebooksTableView" layoutX="14.0" layoutY="90.0" prefHeight="537.0"
                 prefWidth="1004.0">
         <columns>
            <TableColumn id="ebooksNameColumn" fx:id="ebooksNameColumn" prefWidth="252.0"
                         text="Name"/>
            <TableColumn fx:id="ebooksPriceColumn" prefWidth="72.0" text="Price"/>
            <TableColumn fx:id="ebooksSubcategoryColumn" prefWidth="184.0"
                         text="Subcategory"/>
            <TableColumn fx:id="ebooksCartButtonColumn" prefWidth="213.0"/>
            <TableColumn fx:id="ebooksStarButtonColumn" prefWidth="242.0"/>
            <TableColumn fx:id="ebooksNumberOfOrdersColumn" prefWidth="0" visible="false"/>

         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
         </columnResizePolicy>
      </TableView>
      <Label id="titleLabel" alignment="CENTER" layoutX="350.0" layoutY="6.0" prefHeight="47.0" prefWidth="318.0"
             text="EBOOKS"/>
   </children>
</Pane>

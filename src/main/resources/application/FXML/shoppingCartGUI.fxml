<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchor" onMousePressed="#Dragging" prefHeight="600.0"
            prefWidth="1050.0" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/17"
            fx:controller="application.Controllers.ShoppingCartController">
    <TableView fx:id="cartTableView" layoutX="34.0" layoutY="72.0" prefHeight="503.0" prefWidth="982.0">
        <columns>
            <TableColumn fx:id="cartNameColumn" prefWidth="211.0" text="Name"/>
            <TableColumn fx:id="cartPriceColumn" prefWidth="140.0" text="Price"/>
            <TableColumn fx:id="cartQuantityColumn" prefWidth="350.0" text="Quantity"/>
            <TableColumn fx:id="cartValueColumn" prefWidth="115.0" text="Value"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>
